{
    "version": 2,   
    "routes": [
        {
            "src": "/(.*)",
            "headers": {
                "X-Frame-Options": "ALLOW-FROM '*'",
                "X-XSS-Protection": "1; mode=block",
                "X-Content-Type-Options": "nosniff",                
                "Referrer-Policy": "no-referrer",                
                "Feature-Policy": "accelerometer '*'; camera '*'; geolocation '*'; gyroscope '*'; magnetometer 'none'; microphone '*'; payment '*'; usb 'none'"
            }
        }
    ]
}